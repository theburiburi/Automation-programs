{
  "manifest_version": 3,
  "name": "SSAFY Auto Checkout Helper",
  "version": "1.7",
  "permissions": [
    "tabs",
    "alarms",
    "storage",
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "https://edu.ssafy.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}